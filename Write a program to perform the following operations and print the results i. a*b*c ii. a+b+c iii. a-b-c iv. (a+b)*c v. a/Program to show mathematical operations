#include <stdio.h>
int main()
{
    int a,b,c , sum, product, difference, quotient, x;
    printf ("Enter the value of a \n");
    scanf ("%d", &a);
    printf ("Enter the value of b\n");
    scanf ("%d", &b);
    printf ("Enter the value of c\n");
    scanf ("%d", &c);
    sum=a+b+c;
    printf("%d+%d+%d=%d\n", a, b, c, sum);
    product=a*b*c;
    printf("%d*%d*%d=%d\n", a, b, c, product);
    difference=a-b-c;
    printf("%d-%d-%d=%d\n", a, b, c, difference);
    x=(a+b)*c;
    printf("(%d+%d)*%d=%d\n", a, b, c, x);
    quotient=a/b;
    printf("%d/%d=%d\n", a, b, quotient);
    return 0;
}
